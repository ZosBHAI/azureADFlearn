{
	"name": "CopyPipeline_6j1",
	"properties": {
		"activities": [
			{
				"name": "Copy_6j1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "raw/sampledata/"
					},
					{
						"name": "Destination",
						"value": "base//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"maxConcurrentConnections": 4,
							"recursive": true,
							"wildcardFileName": "*",
							"deleteFilesAfterCompletion": true
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"maxConcurrentConnections": 4,
							"copyBehavior": "PreserveHierarchy",
							"metadata": [
								{
									"name": "last_modified",
									"value": "$$LASTMODIFIED"
								}
							]
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"skipErrorFile": {
						"fileMissing": true,
						"fileForbidden": true,
						"invalidFileName": true,
						"dataInconsistency": false
					},
					"validateDataConsistency": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Info",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "AzureDataLakeStorage1",
								"type": "LinkedServiceReference"
							},
							"path": "dataingestionlogs"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_6j1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_6j1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}